{
  "multipart": [
    {
      "when": {
        "north": "none",
        "east": "none",
        "south": "none",
        "west": "none"
      },
      "apply": {
        "model": "nekoma-tweaks:block/bluestone_wire_dot"
      }
    },
    {
      "when": {
        "north": "side|up"
      },
      "apply": {
        "model": "nekoma-tweaks:block/bluestone_wire_side0"
      }
    },
    {
      "when": {
        "south": "side|up"
      },
      "apply": {
        "model": "nekoma-tweaks:block/bluestone_wire_side0",
        "y": 180
      }
    },
    {
      "when": {
        "east": "side|up"
      },
      "apply": {
        "model": "nekoma-tweaks:block/bluestone_wire_side0",
        "y": 90
      }
    },
    {
      "when": {
        "west": "side|up"
      },
      "apply": {
        "model": "nekoma-tweaks:block/bluestone_wire_side0",
        "y": 270
      }
    },
    {
      "when": {
        "north": "up"
      },
      "apply": {
        "model": "nekoma-tweaks:block/bluestone_wire_side1"
      }
    },
    {
      "when": {
        "south": "up"
      },
      "apply": {
        "model": "nekoma-tweaks:block/bluestone_wire_side1",
        "y": 180
      }
    },
    {
      "when": {
        "east": "up"
      },
      "apply": {
        "model": "nekoma-tweaks:block/bluestone_wire_side1",
        "y": 90
      }
    },
    {
      "when": {
        "west": "up"
      },
      "apply": {
        "model": "nekoma-tweaks:block/bluestone_wire_side1",
        "y": 270
      }
    }
  ]
}
